.Box
	padding 0.1em
	display flex
	margin 0.1em
	&-id	
		font-size 0.8em
		justify-content space-between
		width 85px
		color white
		margin 0 0 0 0.5em
		text-align center
	&-description	
		font-size 0.8em
		justify-content space-between
		width 210px
		color white
		margin 0 0 0 0.5em
	&-order
		justify-content space-between	
		width 80px
		color white
		background rgba(4, 4, 27, 0.97)
		border 0.1em solid rgba(127, 127, 185, 1)
		box-shadow 0 0 10px rgba(72, 73, 78, 0.5)
		border-radius 0.5em
		margin 0 0 0 0.9em
	&-delivery
		justify-content space-between	
		width 80px
		color white
		background rgba(4, 4, 27, 0.97)
		border 0.1em solid rgba(127, 127, 185, 1) 
		box-shadow 0 0 10px rgba(72, 73, 78, 0.5)	
		border-radius 0.5em
		margin 0 0 0 0.5em
	&-fulfillment
		justify-content space-between	
		width 80px
		color white
		background rgba(4, 4, 27, 0.97)
		border 0.1em solid rgba(127, 127, 185, 1)
